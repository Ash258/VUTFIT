' Ackerman function for ackerman(2, 5)

function ackermann(m as integer, n as integer) as integer
	if m = 0 then
		return n + 1
	else
		if n = 0 then
			return ackermann(m - 1, 1)
		else
			return ackermann(m - 1, ackermann(m, n - 1))
		end if
	end if
end function

scope
    dim a as integer
    a = ackermann(2, 5)
    print a;
end scope
